[push]
  default = current
[color]
  ui = auto
[alias]
  aa = add .
  b = branch
  br = branch
  cb = checkout -b
  cm = commit --verbose
  co = checkout
  l = log --decorate --graph --oneline --date-order
  st = status
[core]
  excludesfile = ~/.gitignore
[fetch]
  prune = true
[user]
  email = alan.tolearn@gmail.com
